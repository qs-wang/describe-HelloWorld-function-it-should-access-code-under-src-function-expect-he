{
  "name": "jasmine-karma-nodejs-boilerplate",
  "version": "0.0.1",
  "description": "A boiler plate project using karma-browserify for running jasmine tests with karma, and node. Support ES6, and CommonJS.",
  "main": "src/HelloWorld.js",
  "scripts": {
    "test": "karma start",
    "test:watch": "karma start --no-single-run --auto-watch"
  },
  "keywords": [
    "karma-browserify"
  ],
  "author": "Q.S. Wang <wangqs_eclipse@yahoo.com>",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babelify": "^7.3.0",
    "browserify": "^12.0.2",
    "karma": "^0.13.0",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-browserify": "^5.1.1",
    "karma-jasmine": "^0.1.5",
    "karma-phantomjs-launcher": "^0.1.4",
    "watchify": "^3.9.0"
  },
  "browserify-shim": {}
}
