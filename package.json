{
  "name": "jasmine-karma-nodejs-boilerplate",
  "version": "0.0.1",
  "description": "A boilerplate project using karma-browserify for running jasmine tests with karma, and node. Support ES6, and CommonJS.",
  "main": "src/HelloWorld.js",
  "scripts": {
    "test": "karma start",
    "test:watch": "karma start --no-single-run --auto-watch"
  },
  "keywords": [
    "karma-browserify"
  ],
  "author": "Q.S. Wang <wangqs_eclipse@yahoo.com>",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-browserify": "^5.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "phantomjs": "^2.1.7",
    "watchify": "^3.9.0"
  }
}
